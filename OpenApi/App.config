        <listeners>
          <add name="xml" />
        </listeners>
      </source>
      <source name="System.ServiceModel.MessageLogging">
        <listeners>
          <add name="xml" />
        </listeners>
      </source>
    </sources>

    <sharedListeners>
      <add initializeData="C:\ice.svclog" type="System.Diagnostics.XmlWriterTraceListener"
        name="xml" />
